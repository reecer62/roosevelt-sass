{
  "name": "roosevelt-sass",
  "description": "SASS CSS preprocessor support for Roosevelt MVC web framework",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/rooseveltframework/roosevelt-sass/graphs/contributors"
    }
  ],
  "version": "0.4.1",
  "homepage": "https://github.com/rooseveltframework/roosevelt-sass",
  "license": "CC-BY-4.0",
  "main": "roosevelt-sass.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=10.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "node-sass": "4.12.0"
  },
  "devDependencies": {
    "c8": "~5.0.1",
    "codecov": "~3.5.0",
    "eslint": "~5.16.0",
    "eslint-plugin-mocha": "~5.3.0",
    "fs-extra": "~8.0.1",
    "husky": "~2.4.1",
    "lint-staged": "~8.2.1",
    "mocha": "~6.1.4",
    "roosevelt": "~0.14.4",
    "standard": "~13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/roosevelt-sass.git"
  },
  "keywords": [
    "roosevelt.js",
    "node-sass",
    "node.js",
    "sass",
    "css",
    "preprocessor"
  ],
  "preferGlobal": true,
  "_from": "roosevelt-sass@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "scripts": {
    "codecov": "./node_modules/.bin/codecov",
    "coverage": "c8 --reporter=text --reporter=lcov mocha --timeout 25000 test/unit",
    "lint": "standard && eslint ./test/unit",
    "test": "mocha --timeout 25000 test/unit"
  },
  "lint-staged": {
    "*.js": "standard && eslint ./test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && eslint ./test"
    }
  }
}
